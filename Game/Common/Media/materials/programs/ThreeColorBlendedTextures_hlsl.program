vertex_program ThreeColorBlendedTextures_vs hlsl
{
	source ThreeColorBlendedTextures_vs.hlsl
	entry_point main
	target vs_2_0
	
	default_params
	{
		param_named_auto worldViewProj worldviewproj_matrix
		param_named_auto Time time_0_x 60
	}
}

fragment_program ThreeColorBlendedTextures_ps hlsl
{
	source ThreeColorBlendedTextures_ps.hlsl
	entry_point main
	target ps_2_0
}
